tag:
type: u32
help: "vlan ID"
syntax: $(@) >= 0 && $(@) <= 4095; "vlan ID must be between 0 and 4095"
create: "modprobe 8021q"; "error loading 802.1q driver"
create: "vconfig add $(../@) $(@)"; "error adding vlan id $(@) to dev $(../@)"
delete: "vconfig rem $(../@).$(@)"; "error removing vlan id $(@) from dev $(../@)"
