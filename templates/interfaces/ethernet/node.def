tag:
type: txt
help: "Ethernet interface name"
syntax: exec " \
        if [ -z \"`ip addr | grep $(@) `\" ]; then \
          echo ethernet interface $(@) doesn\\'t exist on this system ; \
          exit 1 ; \
        fi ; "
update: "sudo ip link set $(@) up" ; "Error enabling dev $(@)"
delete: "sudo ip link set $(@) down" ; "Error disabling dev $(@)"
